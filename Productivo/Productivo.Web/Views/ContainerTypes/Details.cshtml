@model Productivo.Core.Entities.ContainerTypeEntity


<!DOCTYPE html>
<html>
<head>

</head>
<body>

    <div class="row wrapper border-bottom page-heading">
        <div class="col-sm-4">
            <h2>Tipos de Contenedor</h2>
        </div>
    </div>

    <div class="row">
        <div class="col-xl-12">
            <div id="panel-1" class="panel">
                <div class="panel-hdr">
                    <h2></h2>
                    <div class="panel-toolbar">
                        <button class="btn btn-panel" data-action="panel-collapse" data-toggle="tooltip" data-offset="0,10" data-original-title="Minimizar"></button>
                        <button class="btn btn-panel" data-action="panel-fullscreen" data-toggle="tooltip" data-offset="0,10" data-original-title="Maximizar"></button>
                        <button class="btn btn-panel" data-action="panel-close" data-toggle="tooltip" data-offset="0,10" data-original-title="Cerrar"></button>
                    </div>
                </div>
                <div class="panel-container show">
                    <div class="panel-content">

                        <div class="row">
                            <div class="col-lg-8">
                                <div class="m-b-md">
                                    <h2>Información</h2>
                                </div>
                                <div>
                                    <hr />
                                    <dl class="row">
                                        <dt class="col-sm-4">
                                            @Html.DisplayNameFor(model => model.Name)
                                        </dt>
                                        <dd class="col-sm-8">
                                            @Html.DisplayFor(model => model.Name)
                                        </dd>
                                        <dt class="col-sm-4">
                                            @Html.DisplayNameFor(model => model.Lenght)
                                        </dt>
                                        <dd class="col-sm-8">
                                            @Html.DisplayFor(model => model.Lenght)
                                        </dd>
                                        <dt class="col-sm-4">
                                            @Html.DisplayNameFor(model => model.Width)
                                        </dt>
                                        <dd class="col-sm-8">
                                            @Html.DisplayFor(model => model.Width)
                                        </dd>
                                        <dt class="col-sm-4">
                                            @Html.DisplayNameFor(model => model.Height)
                                        </dt>
                                        <dd class="col-sm-8">
                                            @Html.DisplayFor(model => model.Height)
                                        </dd>
                                        <dt class="col-sm-4">
                                            @Html.DisplayNameFor(model => model.Weight)
                                        </dt>
                                        <dd class="col-sm-8">
                                            @Html.DisplayFor(model => model.Weight)
                                        </dd>
                                        <dt class="col-sm-4">
                                            @Html.DisplayNameFor(model => model.MaxLoadCM)
                                        </dt>
                                        <dd class="col-sm-8">
                                            @Html.DisplayFor(model => model.MaxLoadCM)
                                        </dd>
                                        <dt class="col-sm-4">
                                            @Html.DisplayNameFor(model => model.MaxLoadKG)
                                        </dt>
                                        <dd class="col-sm-8">
                                            @Html.DisplayFor(model => model.MaxLoadKG)
                                        </dd>
                                    </dl>
                                </div>
                                <div>
                                    <a asp-action="Update" asp-route-id="@Model.Id" class="btn btn-success btn-sm">Editar</a> |
                                    <a asp-action="Index" class="h3-Pandora  btn btn-secondary btn-sm">Regresar</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>



@section ScriptsBlock {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

